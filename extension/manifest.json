{
	"name": "roblox-sveltekit",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
	  	"service_worker": "background.js"
	},
	"content_scripts": [{
		"js": ["content.js"],
		"matches": ["http://localhost:5173/*", "http://localhost:4173/*", "https://svelteblox.vercel.app/*"]
	}],
	"host_permissions": ["https://*.roblox.com/*"]
}